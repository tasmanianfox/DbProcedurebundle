{
  "name":        "tfox/db-procedure-bundle",
  "type":        "symfony-bundle",
  "description": "Execution of stored procedures. Uses annotated classes to define procedures in Symfony.",
  "keywords":    ["oci0", "oracle", "procedure", "symfony", "bundle"],
  "homepage":    "http://github.com/tasmanianfox/DbProcedureBundle",
  "license":     "MIT",

  "authors": [
    {
      "name": "Sergei Belyakov",
      "homepage": "http://tfox.eu"
    }
  ],

  "require": {
    "php":                      ">=5.3.2",
    "symfony/framework-bundle": ">=2.4.0"
  },

  "minimum-stability": "dev",

  "autoload": {
    "psr-0": {
      "TFox\\DbProcedureBundle": ""
    }
  },

  "target-dir": "TFox/DbProcedureBundle",

  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}